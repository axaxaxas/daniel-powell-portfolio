<h1>List of Reports</h1>

<table>
  <tr>
    <th>|Source|</th>
    <th>|Markov model|</th>
    <th>|Probability|</th>
    <th>|Universe Probability|</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @reports.each do |report| %>
  <tr>
    <td><%= report.source %></td>
    <td><%= report.markov %></td>
    <td><%= report.pval %></td>
    <td><%= report.universepval %></td>
    <td><%= link_to 'Show', report %></td>
    <td><%= link_to 'Edit', edit_report_path(report) %></td>
    <td><%= link_to 'Destroy', report, :method => :delete, :data => { :confirm => 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

